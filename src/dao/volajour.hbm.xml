<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="dao.volajour" table="volajour" >

	<id name="numVol" column="numVol"  >
		
	</id>
	
	
	<property name="dateVol" column="dateVol"></property>
	<property name="heureEstime" column="heureEstime"></property>
	<property name="statutChange" column="statutChange"></property>

</class>
 
  
</hibernate-mapping>