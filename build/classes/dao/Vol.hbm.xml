<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="dao.Vol" table="Vol" >

	
	<composite-id class="dao.Vol" mapped="true">         
	<key-property name="numVol" column="numVol" />        
	 <key-property name="dateVol" column="dateVol"/>
    </composite-id >
	
	<property name="codeAerDepart" column="codeAerDepart"></property>
	<property name="aeroportDestination" column="aeroportDestination"></property>
		<property name="dateVol" column="dateVol"></property>
	<property name="heurePanifie" column="heurePanifie"></property>
	<property name="heureEstime" column="heureEstime"></property>
	<property name="statutChange" column="statutChange"></property>
	<property name="idCompagnie" column="idCompagnie"></property>
	<property name="idVille" column="idVille"></property>
	
	<property name="volDepart" column="volDepart"></property>
	
	
</class>
 
  
</hibernate-mapping>